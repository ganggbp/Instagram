{
  "analytics": {
    "instagram": {
      "providerPlugin": "awscloudformation",
      "service": "Pinpoint"
    }
  },
  "api": {
    "Instagram": {
      "dependsOn": [
        {
          "attributes": [
            "UserPoolId"
          ],
          "category": "auth",
          "resourceName": "Instagram"
        }
      ],
      "output": {
        "authConfig": {
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "AWS_IAM"
            }
          ],
          "defaultAuthentication": {
            "authenticationType": "AMAZON_COGNITO_USER_POOLS",
            "userPoolConfig": {
              "userPoolId": "authInstagram"
            }
          }
        }
      },
      "providerPlugin": "awscloudformation",
      "service": "AppSync"
    }
  },
  "auth": {
    "Instagram": {
      "customAuth": false,
      "dependsOn": [
        {
          "attributes": [
            "Arn",
            "Name"
          ],
          "category": "function",
          "resourceName": "InstagramPostConfirmation",
          "triggerProvider": "Cognito"
        }
      ],
      "frontendAuthConfig": {
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": [
          "EMAIL",
          "NAME"
        ],
        "socialProviders": [
          "FACEBOOK",
          "GOOGLE"
        ],
        "usernameAttributes": [
          "EMAIL"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    }
  },
  "function": {
    "FeedGenerator": {
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput"
          ],
          "category": "api",
          "resourceName": "Instagram"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "InstagramPostConfirmation": {
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput"
          ],
          "category": "api",
          "resourceName": "Instagram"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "UserFollowTrigger": {
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput"
          ],
          "category": "api",
          "resourceName": "Instagram"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "parameters": {
    "AMPLIFY_function_FeedGenerator_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "FeedGenerator"
        }
      ]
    },
    "AMPLIFY_function_FeedGenerator_s3Key": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "FeedGenerator"
        }
      ]
    },
    "AMPLIFY_function_InstagramPostConfirmation_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "InstagramPostConfirmation"
        }
      ]
    },
    "AMPLIFY_function_InstagramPostConfirmation_s3Key": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "InstagramPostConfirmation"
        }
      ]
    },
    "AMPLIFY_function_UserFollowTrigger_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "UserFollowTrigger"
        }
      ]
    },
    "AMPLIFY_function_UserFollowTrigger_s3Key": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "UserFollowTrigger"
        }
      ]
    }
  },
  "storage": {
    "storage": {
      "dependsOn": [],
      "providerPlugin": "awscloudformation",
      "service": "S3"
    }
  }
}